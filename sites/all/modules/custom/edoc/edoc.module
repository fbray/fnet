 */
function edoc_get_language_override_id($node) {
  $siteReg = module_invoke('fnet_helper', 'site_region');
  $regLang = $siteReg["regLang"];
  $langNodeId = NULL;

  $validLangNode = array();
  if ((is_array($node->field_edoc_langreg)) && (isset($node->field_edoc_langreg[LANGUAGE_NONE]))){
    foreach ($node->field_edoc_langreg[LANGUAGE_NONE] as $key => $val) {
      foreach ($val as $k => $v) {
        if ($k == "nid") {
          $validLangNode[$v] = 1;
        }
      }
    }
  }

  if (count($validLangNode) > 0) {
    $langNodeId = "";
    $edocLang = views_get_view('admin_edoc_language'); // Get Drupal view by name.
    $edocLang->set_arguments((array) $node->nid); // Set eDoc node id as view argument.
    $edocLang->init_display();
    $edocLang->pre_execute();
    $edocLang->execute();
    $edocLang = $edocLang->result;

    if (count($edocLang) > 0) {
      foreach ($edocLang as $lang) {
        if (isset($validLangNode[$lang->nid]) && substr($lang->field_field_edoc_lang_lng[0]['raw']['value'], 0, 5) == $regLang) {
          $langNodeId = $lang->nid;
          break;
        }
      }
    }
  }

  return $langNodeId;
}
