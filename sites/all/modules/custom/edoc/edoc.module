}

/**
 * Theme hook which themes the appropriate brand banner for print pages only.
 * TODO: Deprecated. Only the Fluke Networks logo needs to be here.
 *
 * @param $variables
 * @return string
 */
function theme_edoc_print_banner($variables) {
  $taxonomy = $variables['taxonomy'];
  $brand_banner = '';
  $edoc_brand = '';
  $edoc_type_vocab = variable_get('edoc_type_vocab', 0);
  $edoc_brands_vocab = variable_get('edoc_brands_vocab', 0);

  foreach ($taxonomy as $term_data) {
    switch ($term_data->vid) {
      case $edoc_type_vocab:
        $edoc_type = $term_data->name;
        break;
      case $edoc_brands_vocab:
        $edoc_brand = $term_data->name;
        break;
      default:
        break;
    }
  }

  // prepare page logo
  $base_dir = drupal_get_path('module', 'edoc') . '/images';
  if (strlen($edoc_brand) > 0) {
    $filename = strtolower(str_replace(' ', '_', $edoc_brand));
    $image_path = $base_dir . '/' . $filename . '.jpg';

    if (file_exists($image_path)) {
      $brand_banner = theme('image', array('path' => $image_path));
    }
    else {
      watchdog('fnet_helper', __LINE__ . ': Missing print logo for brand <em>!brand</em>: !logo', array(
          '!brand' => $edoc_brand,
          '!logo' => $image_path
        ));
    }
  }

  return $brand_banner;
}

