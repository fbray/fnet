<script type="text/javascript">

    var tab-list = new Array("industry_overview", "industry_expertise", "industry_products");

    function suitesToggle(current_tab) {

        var tab-count;
        current_tab = current_tab.toLowerCase();
        current_tab = current_tab.replace(" ", "_");

        for (tab-count in tab-list) {
            temp_box = "box-" + tab-list[tab-count];
            temp_tab = "tab-" + tab-list[tab-count];
            document.getElementById(temp_tab).className = "suites-tab-off";
            document.getElementById(temp_box).style.display = 'none';
        }
        temp_current_box = "box-" + current_tab;
        temp_current_tab = "tab-" + current_tab;
        document.getElementById(temp_current_tab).className = "suites-tab-on";
        document.getElementById(temp_current_box).style.display = '';
    }

</script>
<script language="javascript" type="text/javascript">
    var tab-list = new Array('customers', 'acquisitions', 'products', 'worldwide_presence', 'careers');
</script>
